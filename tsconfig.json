{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "removeComments": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "allowUnreachableCode": false,
        "target": "es2020",
        "noImplicitAny": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "outDir": "./dist",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "incremental": true,
    },
}